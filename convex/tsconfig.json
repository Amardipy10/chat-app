{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "allowJs": true,
    "outDir": "./_generated"
  },
  "include": ["./**/*.ts", "./**/*.tsx", "./_generated/**/*.ts"],
  "exclude": ["node_modules"]
}
